// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Post {
  id                String  @id @default(cuid())
  title             String  @default("")
  slug              String  @unique @default("")
  content           String  @default("")
  avatar1_filesize  Int?
  avatar1_extension String?
  avatar1_width     Int?
  avatar1_height    Int?
  avatar1_id        String?
  avatar2_filesize  Int?
  avatar2_extension String?
  avatar2_width     Int?
  avatar2_height    Int?
  avatar2_id        String?
  avatar3_filesize  Int?
  avatar3_extension String?
  avatar3_width     Int?
  avatar3_height    Int?
  avatar3_id        String?
  avatar4_filesize  Int?
  avatar4_extension String?
  avatar4_width     Int?
  avatar4_height    Int?
  avatar4_id        String?
}

model Service {
  id    String @id @default(cuid())
  title String @default("")
  slug  String @unique @default("")
}

model Test {
  id    String @id @default(cuid())
  title String @default("")
  slug  String @unique @default("")
}

model Team {
  id               String  @id @default(cuid())
  slug             String  @unique @default("")
  postion          String  @default("")
  name             String  @default("")
  content          String  @default("")
  avatar_filesize  Int?
  avatar_extension String?
  avatar_width     Int?
  avatar_height    Int?
  avatar_id        String?
}